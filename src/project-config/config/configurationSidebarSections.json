{
  "settings": {
    "projectName": "ProjectX",
    "openDevToolsByDefault": false,
    "autoSetupTimeoutSeconds": 90,
    "loadingScreenTimeoutSeconds": 20,
    "sidebarDefaultExpanded": false,
    "terminalScrollback": 1000,
    "maxFloatingTerminals": 15,
    "terminalFontSize": 14,
    "configurationDefaultExpanded": true
  },
  "sections": [
    {
      "id": "service-a",
      "title": "Service A",
      "components": {
        "toggle": true,
        "attachToggle": {
          "enabled": true,
          "mutuallyExclusiveWith": ["service-d"]
        },
        "infoButton": true,
        "gitBranch": true,
        "inputField": {
          "id": "debugPort",
          "placeholder": "Enter debug port",
          "default": "5005",
          "visibleWhen": {
            "configKey": "attachState.service-a",
            "hasValue": true
          }
        },
        "modeSelector": {
          "options": ["run", "suspend"],
          "labels": ["Run", "Suspend"],
          "default": "run"
        },
        "customButtons": [
          {
            "id": "runStorybookButton",
            "label": "Run Storybook",
            "commandId": "runStorybook"
          }
        ],
        "subSections": [
          {
            "id": "frontend",
            "title": "Frontend",
            "components": {
              "toggle": true,
              "modeSelector": {
                "options": ["normal", "dev"],
                "labels": ["Normal", "Dev Mode"]
              }
            }
          }
        ]
      }
    },
    {
      "id": "service-b",
      "title": "Service B",
      "components": {
        "toggle": true,
        "infoButton": true,
        "gitBranch": true,
        "deploymentOptions": [
          { "value": "container", "status": "TBD" },
          { "value": "process" }
        ]
      }
    },
    {
      "id": "service-c",
      "title": "Service C",
      "components": {
        "toggle": true,
        "infoButton": true,
        "gitBranch": true,
        "dropdownSelectors": [
          {
            "id": "kubectlContext",
            "command": "node ./ProjectName-Manager/scripts/simulators/dropdown-simulator.js kubectl-contexts",
            "parseResponse": "lines",
            "placeholder": "Select context...",
            "loadingText": "Loading contexts...",
            "errorText": "Error loading contexts",
            "noOptionsText": "No contexts found",
            "dependsOn": "gcloudProject",
            "defaultValue": {
              "contains": "staging"
            }
          }
        ],
        "subSections": [
          {
            "id": "service-c-sub",
            "title": "Service C Sub",
            "components": {
              "toggle": true,
              "modeSelector": {
                "options": ["forwarding", "process", "mock"],
                "labels": ["Forwarding", "Process", "Mock"],
                "default": "mock"
              },
              "dropdownSelectors": [
                {
                  "id": "urlIntelPod",
                  "command": "node ./ProjectName-Manager/scripts/simulators/dropdown-simulator.js kubectl-pods-service-c-sub",
                  "parseResponse": "lines",
                  "placeholder": "Select URL Intel pod (for Forwarding)...",
                  "loadingText": "Loading URL Intel pods...",
                  "errorText": "Error loading URL Intel pods",
                  "noOptionsText": "No URL Intel pods found",
                  "dependsOn": "kubectlContext",
                  "visibleWhen": {
                    "configKey": "mode",
                    "hasValue": "forwarding"
                  }
                }
              ]
            }
          },
          {
            "id": "threat-intelligence-sub",
            "title": "Threat Intelligence",
            "components": {
              "toggle": true,
              "dropdownSelectors": [
                {
                  "id": "threatIntelPod",
                  "command": "node ./ProjectName-Manager/scripts/simulators/dropdown-simulator.js kubectl-pods-threat-intelligence-sub",
                  "parseResponse": "lines",
                  "placeholder": "Select Threat Intel pod...",
                  "loadingText": "Loading Threat Intel pods...",
                  "errorText": "Error loading Threat Intel pods",
                  "noOptionsText": "No Threat Intel pods found",
                  "dependsOn": "kubectlContext"
                }
              ]
            }
          }
        ]
      }
    },
    {
      "id": "service-d",
      "title": "Service D",
      "components": {
        "toggle": true,
        "attachToggle": {
          "enabled": true,
          "mutuallyExclusiveWith": ["service-a"]
        },
        "infoButton": true,
        "gitBranch": true,
        "inputField": {
          "id": "debugPort",
          "placeholder": "Enter debug port",
          "default": "5005",
          "visibleWhen": {
            "configKey": "attachState.service-d",
            "hasValue": true
          }
        },
        "mainToggle": {
          "validationRequired": {
                        "condition": "gcloudProject && !projectSelectorFallbacks.includes(gcloudProject)",
              "errorMessage": "Cannot enable Service D until a cloud project is selected."
          }
        },
        "modeSelector": {
          "options": ["run", "suspend"],
          "labels": ["Run", "Suspend"],
          "default": "suspend"
        }
      }
    },
    {
      "id": "service-e",
      "title": "Service E",
      "components": {
        "toggle": true,
        "infoButton": true,
        "gitBranch": true,
        "deploymentOptions": true
      }
    },
    {
      "id": "service-f",
      "title": "Service F",
      "testSection": true,
      "components": {
        "toggle": true,
        "infoButton": true,
        "gitBranch": true,
        "modeSelector": {
          "options": [
            { "value": "development" },
            { "value": "staging" },
            { "value": "production" },
            { "value": "mock", "status": "TBD" }
          ],
          "default": "development"
        },
        "subSections": [
          {
            "id": "data-processor",
            "title": "Data Processor",
            "components": {
              "toggle": true,
              "modeSelector": {
                "options": ["batch", "stream", "hybrid"]
              }
            }
          },
          {
            "id": "ml-engine",
            "title": "ML Engine",
            "components": {
              "toggle": true,
              "modeSelector": {
                "options": ["tensorflow", "pytorch", "sklearn"],
                "labels": ["TensorFlow", "PyTorch", "Scikit-learn"]
              },
              "customButton": {
                "id": "mlEngineLogs",
                "label": "ML Logs",
                "commandId": "mlEngineLogCommand"
              }
            }
          }
        ],
        "customButton": {
          "id": "testAnalyticsLogs",
          "label": "View Logs",
          "commandId": "testAnalyticsLogCommand"
        }
      }
    }
  ] 
}