  {
    "id": "activity-logger",
    "conditions": {
      "enabled": true,
      "attachState.activity-logger": true,
      "mode": "run"
    },
    "command": {
      "base": "nvm use ${nodeVersion} && cd ./activity-logger && ./gradlew bootRun -Dspring-boot.run.jvmArguments='-agentlib:jdwp=transport=dt_socket,server=y,suspend=n,address=5005' -Dspring.profiles.active=dev -Dgcloud.project=${gcloudProject}",
      "tabTitle": "Activity Logger (Debug Run)"
    }
  },
  {
    "id": "rule-engine",
    "conditions": {
      "enabled": true,
      "deploymentType": "process"
    },
    "command": {
      "base": "cd rule-engine && ./out/rule-engine",
